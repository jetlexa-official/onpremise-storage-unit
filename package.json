{
  "name": "onpremise-storage-unit",
  "version": "1.0.0",
  "description": "Monolith API Mechanism and the services for Jetlexa.com",
  "main": "index.ts",
  "repository": "https://github.com/jetlexa-official/api-monolith.jetlexa.com.git",
  "author": "Can Gökçeaslan <48398625+cangokceaslan@users.noreply.github.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "npx ts-node",
    "dev": "nodemon --watch src src/index.ts DEVELOPMENT",
    "production": "ts-node ./src/index.ts PRODUCTION",
    "build": "tsc -p tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./build",
    "build:run": "npm run build && npm run production",
    "pm2:start": "npx pm2 start 'node build/index.js PRODUCTION'",
    "pm2:restart": "npx pm2 restart 'node build/index.js PRODUCTION' --update-env",
    "start:production": "npm install && npm run build && npm run pm2:start",
    "lint": "tslint -c tslint.json --project tsconfig.json",
    "test": "jest --config jest.config.js",
    "watch": "npm run build && npm run start",
    "watch:lint": "npm run lint -- --watch",
    "watch:test": "npm run test -- --watch",
    "watch:build": "npm run build -- --watch",
    "watch:start": "npm run start -- --watch"
  },
  "dependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.3",
    "@types/favicon": "file:./@types/favicon",
    "@types/mime-types": "^2.1.1",
    "@types/morgan": "^1.9.3",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "aws-sdk": "^2.1193.0",
    "axios": "^0.26.1",
    "base64-stream": "^1.0.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dir-to-json": "^1.0.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "express-fileupload": "^1.4.0",
    "favicon": "^0.0.2",
    "helmet": "^5.0.2",
    "http-status": "^1.5.0",
    "mime-types": "^2.1.35",
    "moment": "^2.29.3",
    "moment-timezone": "^0.5.34",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pm2": "^5.2.0",
    "tsconfig-paths": "^3.13.0",
    "typescript": "^4.6.2",
    "typescript-transform-paths": "^3.3.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/base64-stream": "^1.0.2",
    "@types/multer": "^1.4.7",
    "@types/node-cron": "^3.0.4",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "tsc-hooks": "^1.1.1"
  }
}
